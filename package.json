{
  "name": "mem-collection",
  "version": "0.5.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "run-p eleventy:dev sass:dev js:dev js:dev:legacy",
    "start:ci": "run-p eleventy:ci sass:dev js:dev js:dev:legacy",
    "build": "run-s eleventy:build sass:build js:build js:build:legacy",
    "build:prod": "run-s eleventy:build:prod sass:build js:build js:build:legacy",
    "build:ci": "run-s eleventy:build:ci sass:build js:build js:build:legacy",
    "eleventy:dev": "NODE_ENV=development eleventy --serve",
    "eleventy:ci": "NODE_ENV=ci eleventy --serve",
    "eleventy:build": "NODE_ENV=development eleventy",
    "eleventy:build:prod": "NODE_ENV=production eleventy",
    "eleventy:build:ci": "NODE_ENV=ci eleventy",
    "sass:dev": "sass --watch --embed-source-map src/scss:dist/css",
    "sass:build": "sass --no-source-map --style=compressed src/scss:dist/css",
    "clean": "rm -rf dist",
    "lint": "run-s lint:html lint:css lint:js",
    "pa11y-ci": "pa11y-ci -c .pa11yci.json -s http://localhost:8080/test-sitemap.xml --sitemap-find \"https://www.memcollection.com/\" --sitemap-replace \"http://localhost:8080/\"",
    "lint:html": "npx start-server-and-test eleventy:dev 8080 pa11y-ci",
    "lint:css": "stylelint 'src/*css/**/*.*css' --color --formatter verbose",
    "js:dev": "esbuild src/js/*.ts --format=esm --bundle=true --splitting=true --outdir=dist/js --watch --sourcemap=inline",
    "js:build": "esbuild src/js/*.ts --format=esm --bundle=true --splitting=true --outdir=dist/js --minify=true",
    "js:dev:legacy": "NODE_ENV=development webpack --progress -w",
    "js:build:legacy": "NODE_ENV=production webpack --progress",
    "lint:js": "eslint . --ext .js,.ts",
    "lint:js:fix": "eslint . --fix --ext .js,.ts",
    "test": "run-s test:jest test:cypress",
    "test:cypress": "npx cypress run",
    "test:jest": "jest --coverage --maxWorkers=1",
    "update-deps": "ncu -u",
    "postupdate-deps": "npm install"
  },
  "keywords": [],
  "author": "Megan McCarty",
  "license": "ISC",
  "browserslist": [
    ">0%",
    "not dead",
    "not supports es6-module"
  ],
  "dependencies": {
    "@11ty/eleventy-fetch": "^5.1.1",
    "cypress": "^15.8.2",
    "cypress-axe": "^1.7.0",
    "dotenv": "^17.2.3",
    "leaflet": "^1.9.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-server-browsersync": "^1.0.2",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-typescript": "^7.28.5",
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.21",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "axe-core": "^4.11.1",
    "babel-jest": "^30.2.0",
    "babel-loader": "^10.0.0",
    "esbuild": "^0.27.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "npm-check-updates": "^19.3.1",
    "npm-run-all": "^4.1.5",
    "pa11y-ci": "^4.0.1",
    "sass": "^1.97.2",
    "start-server-and-test": "^2.1.3",
    "stylelint": "^16.26.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "type": "module"
}
